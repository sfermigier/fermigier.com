<head>
  <meta charset="utf-8"/>

  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible"/>

  <meta
    content="width=device-width, initial-scale=1, shrink-to-fit=no"
    name="viewport"
  />

  <title>Stefane Fermigier, tech entrepreneur</title>

  <meta
    property="og:title"
    content="Stefane Fermigier, tech entrepreneur"
  />

  <meta property="og:locale" content="en_US"/>

  <link rel="canonical" href="//post"/>

  <meta property="og:url" content="//post"/>

  <meta
    name="description"
    content="Stefane Fermigier, tech entrepreneur"
  />

  <link rel="icon" type="image/png" href="/assets/img/favicon.png"/>

  {%- if og_data %}
    {% for k, v in og_data.items() %}
      {% if k.startswith('og:') -%}
        <meta property="{{ k }}" content="{{ v }}"/>
      {%- elif k.startswith('twitter:') -%}
        <meta name="{{ k }}" content="{{ v }}"/>
      {%- endif -%}
    {% endfor %}
  {%- endif %}

  <link
    crossorigin="anonymous"
    href="https://fonts.gstatic.com"
    rel="preconnect"
  />

  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito+Sans:400,700"/>
  <link rel="stylesheet" href="/assets/css/styles.css"/>
</head>
